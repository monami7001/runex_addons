<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<template id="assets_frontend" inherit_id="website.assets_frontend" name="Shop Product Options">
  <xpath expr="." position="inside">
      <script type="text/javascript" src="/website_sale_options_enable/static/src/js/website_sale.js"></script>
  </xpath>
</template>

<template id="modal" inherit_id="website_sale_options.modal" name="Shop Product Options button">
  <xpath expr="//a[@class='btn btn-default a-submit js_goto_shop']" position="attributes">
      <attribute name="class" >btn btn-primary a-submit js_goto_shop</attribute>
  </xpath>
</template>

</data>
</openerp>
